{
  "version": 2,
  "name": "xhs-queue-manager-frontend",
  "builds": [
    {
      "src": "xhs-queue-server/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/xhs-queue-server/static/dashboard.html"
    },
    {
      "src": "/app.js",
      "dest": "/xhs-queue-server/static/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/xhs-queue-server/static/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}