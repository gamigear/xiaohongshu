{
  "manifest_version": 3,
  "name": "XHS Link Collector",
  "version": "1.0",
  "description": "Thu thập link Xiaohongshu để tải sau",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["https://xiaohongshu.vercel.app/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
